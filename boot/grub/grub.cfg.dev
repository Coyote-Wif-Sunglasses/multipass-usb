submenu "systemrescuecd - Alternative Boot Menu" {
    menuentry "SystemRescueCd: all files cached to memory (docache)" {
	    set isofile="$isopath/systemrescuecd-x86-3.3.0.iso"
	    loopback loop $isofile
	    linux (loop)/isolinux/rescue32 isoloop=$isofile setkmap=us scandelay=1 docache
	    initrd (loop)/isolinux/initram.igz
    }

    menuentry "SystemRescueCd: framebuffer console in high resolution" {
	    set isofile="$isopath/systemrescuecd-x86-3.3.0.iso"
	    loopback loop $isofile
	    linux (loop)/isolinux/rescue32 isoloop=$isofile setkmap=us scandelay=1 nomodeset vga=791
	    initrd (loop)/isolinux/initram.igz
    }

    menuentry "SystemRescueCd: directly start the graphical environment" {
	    set isofile="$isopath/systemrescuecd-x86-3.3.0.iso"
	    loopback loop $isofile
	    linux (loop)/isolinux/rescue32 isoloop=$isofile setkmap=us scandelay=1 dostartx
	    initrd (loop)/isolinux/initram.igz
    }

    menuentry "SystemRescueCd: 64bit kernel with default options" {
	    set isofile="$isopath/systemrescuecd-x86-3.3.0.iso"
	    loopback loop $isofile
	    linux (loop)/isolinux/rescue64 isoloop=$isofile setkmap=us scandelay=1
	    initrd (loop)/isolinux/initram.igz
    }

    menuentry "SystemRescueCd: 64bit kernel with all files cached to memory (docache)" {
	    set isofile="$isopath/systemrescuecd-x86-3.3.0.iso"
	    loopback loop $isofile
	    linux (loop)/isolinux/rescue64 isoloop=$isofile setkmap=us scandelay=1 docache
	    initrd (loop)/isolinux/initram.igz
    }

    menuentry "SystemRescueCd: Boot an exising Linux OS installed on the disk (missing grub)" {
	    set isofile="$isopath/systemrescuecd-x86-3.3.0.iso"
	    loopback loop $isofile
	    linux (loop)/isolinux/rescue64 isoloop=$isofile scandelay=1 root=auto
	    initrd (loop)/isolinux/initram.igz
    }

    menuentry "NTPASSWD: Reset or edit Windows passwords" {
	    set isofile="$isopath/systemrescuecd-x86-3.3.0.iso"
	    loopback loop $isofile
	    linux (loop)/ntpasswd/vmlinuz isoloop=$isofile rw
	    initrd (loop)/ntpasswd/initrd.cgz (loop)/ntpasswd/scsi.cgz
    }

    menuentry "MHDD: Low-level Hard Drive diagnostic tool" {
	    set isofile="$isopath/systemrescuecd-x86-3.3.0.iso"
	    loopback loop $isofile
	    linux16 /boot/grub/memdisk floppy
	    initrd16 (loop)/bootdisk/mhdd.img
    }
}


menuentry "FreeNAS-8.0.4-RELEASE-x64" {
	#insmod zfs
	insmod ufs2
	insmod iso9660

	# /usr/local/sbin/grub-probe -d /dev/sde1 -t fs_uuid
	search --no-floppy --fs-uuid --set=root ${drive_uuid}

	set isofile="$isopath/FreeNAS-8.0.4-RELEASE-x64.iso"
	loopback loop $isofile
	kfreebsd  (loop)/boot/kernel/kernel.gz
	kfreebsd_loadenv  (loop)/boot/device.hints

	#set FreeBSD.vfs.root.mountfrom=ufs:/dev/ad4s1a
	set kFreeBSD.vfs.root.mountfrom=cd9660:/dev/acd0
	set kFreeBSD.vfs.root.mountfrom.options=rw
}



