menuentry "ubuntu-12.04.3-server-amd64.iso" {
	set isofile="$isopath/ubuntu-12.04.3-server-amd64.iso"
	loopback loop $isofile 
	set gfxpayload=keep
	linux   (loop)/install/vmlinuz file=/cdrom/preseed/ubuntu-server.seed iso-scan/filename=$isofile quiet --
	initrd  (loop)/install/initrd.gz
}

menuentry "manjaro-mate-0.8.6-x86_64.iso" {
	set isofile="$isopath/manjaro-mate-0.8.6-x86_64.iso"
	loopback loop $isofile
	linux (loop)/manjaro/boot/x86_64/manjaroiso img_dev=/dev/disk/by-label/$drive_label img_loop=$isofile misobasedir=manjaro misolabel=MJRO_086 nouveau.modeset=0 i915.modeset=1 radeon.modeset=0 logo.nologo locale.LANG=en_AU.utf8 timezone=Australia/Melbourne nonfree=yes --
	initrd (loop)/manjaro/boot/x86_64/manjaro.img
}

